{
  "semi": false,                  
  "printWidth": 80,              
  "tabWidth": 2,                 
  "useTabs": false,              
  "singleQuote": true,          
  "quoteProps": "as-needed",    
  "jsxSingleQuote": false,      
  "trailingComma": "all",       
  "bracketSpacing": true,       
  "jsxBracketSameLine": false,  
  "arrowParens": "always",      
  "proseWrap": "preserve",      
  "endOfLine": "lf"             
}
